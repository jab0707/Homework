file = {:D$\backslash$:/Users/Jake/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Parfrey et al. - 1996 - Nephrology Dialysis Transplantation Outcome and risk factors for left ventricular disorders in chronic uraemia.pdf:pdf},
