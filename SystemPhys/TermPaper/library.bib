number = {1 Pt 2},
pages = {H301--21},
pmid = {9688927},
title = {{Ionic mechanisms underlying human atrial action potential properties: insights from a mathematical model.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/9688927},
volume = {275},
year = {1998}
}
