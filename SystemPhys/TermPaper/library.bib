number = {4},
pages = {725--733},
publisher = {Elsevier Inc.},
title = {{Image-based modeling of acute myocardial ischemia using experimentally derived ischemic zone source representations}},
url = {https://doi.org/10.1016/j.jelectrocard.2018.05.005},
