@Article{BMB:Dur70,
  author =       "D. Durrer and R.T. van Dam and G.E. Freud and       
  M.J. Janse and F.L. Meijler and R.C. Arzbaecher",
  title =        "Total excitation of the isolated human heart",
  journal =      "Circ.",
