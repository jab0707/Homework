@Article{BMB:Han2003,
  author =       "F Hanser and M Seger and B Tilg and R Modre and G
                 Fischer and B Messnarz and F Hintringer and T Berger
                 and F X Roithinger",
  title =        "Influence of ischemic and infarcted tissue on the
