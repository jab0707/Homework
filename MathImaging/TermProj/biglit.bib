@Article{RSM:Ere2011b,
  author =       "B. Erem and D.H. Brooks and P.M. van Dam and J.G.
                 Stinstra and R.S. MacLeod",
  title =        "Spatiotemporal estimation of activation times of
                 fractionated ECGs on complex heart surfaces.",
