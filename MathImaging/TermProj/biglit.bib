@Article{RSM:Cap2005a,
  author =       "R. Cappato and H. Calkins and S. A. Chen and W. Davies
                 and Y. Iesaka and J. Kalman and Y. H. Kim and G. Klein
                 and D. Packer and A. Skanes",
